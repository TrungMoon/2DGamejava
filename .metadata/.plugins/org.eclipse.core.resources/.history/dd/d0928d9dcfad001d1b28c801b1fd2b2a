package main;

import object.OBJ_Door;
import object.OBJ_Key;

public class AssetSetter {

	GamePanel gp;
	
	public AssetSetter(GamePanel gp) {
		this.gp = gp;
	}
	public void setObject() {
		
		gp.obj[0] = new OBJ_Key();
		gp.obj[0].worldX = 23 * gp.titleSize;
		gp.obj[0].worldY = 7 * gp.titleSize;
		
		gp.obj[1] = new OBJ_Key();
		gp.obj[1].worldX = 23 * gp.titleSize;
		gp.obj[1].worldY = 40 * gp.titleSize;
		
		gp.obj[2] = new OBJ_Key();
		gp.obj[2].worldX = 37 * gp.titleSize;
		gp.obj[2].worldY = 7 * gp.titleSize;
		
		gp.obj[3] = new OBJ_Door();
		gp.obj[3].worldX =  * gp.titleSize;
		gp.obj[3].worldY =  * gp.titleSize;
		
		gp.obj[4] = new OBJ_Door();
		gp.obj[4].worldX =  * gp.titleSize;
		gp.obj[4].worldY =  * gp.titleSize;
		
		gp.obj[5] = new OBJ_Door();
		gp.obj[5].worldX =  * gp.titleSize;
		gp.obj[5].worldY =  * gp.titleSize;
		
		gp.obj[5] = new OBJ_Door();
		gp.obj[5].worldX =  * gp.titleSize;
		gp.obj[5].worldY =  * gp.titleSize;
		
		gp.obj[5] = new OBJ_Door();
		gp.obj[5].worldX =  * gp.titleSize;
		gp.obj[5].worldY =  * gp.titleSize;
		
		gp.obj[5] = new OBJ_Door();
		gp.obj[5].worldX =  * gp.titleSize;
		gp.obj[5].worldY =  * gp.titleSize;
		
	}
}
